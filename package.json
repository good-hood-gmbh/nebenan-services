{
  "name": "nebenan-services",
  "description": "A collection of client services",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
  "repository": "good-hood-gmbh/nebenan-services",
  "bugs": "https://github.com/good-hood-gmbh/nebenan-services/issues",
  "version": "2.0.0",
  "engines": {
    "node": "^10.x.x"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm test && npm run lint && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "clean": "rm -rf ./lib/ || exit 1",
    "build:babel": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "prepublishOnly": "npm run check && npm run clean && npm run build:babel",
    "lint": "eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "test": "npm run build:babel && TZ='GMT' mocha \"test/**/*.js\"",
    "test:tofile": "npm run build:babel && TZ='GMT' mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.0.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "chai": "^4.2.0",
    "date-fns": "^1.30.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "local-storage-adapter": "^2.0.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "nebenan-helpers": "^3.1.0",
    "nebenan-redux-tools": "^2.0.0",
    "normalizr": "^3.3.0",
    "sinon": "^7.2.2",
    "updeep": "^1.1.0"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.0.0",
    "local-storage-adapter": "^2.0.0",
    "updeep": "^1.1.0",
    "date-fns": "^1.30.1",
    "lodash": "^4.17.11",
    "nebenan-helpers": "^3.1.0",
    "nebenan-redux-tools": "^2.0.0",
    "normalizr": "^3.3.0"
  }
}
